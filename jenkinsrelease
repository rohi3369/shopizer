pipeline {
    agent any
    triggers {
           cron('* 23 * * *')
    }
    stages {
        stage ('vcs') {
            steps{
                git url: 'https://github.com/rohi3369/shopizer.git'
            }
        } 
        stage(build){
            steps
        sh 'mvn clean install'
        }
    }
}
     